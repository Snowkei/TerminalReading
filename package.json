{
  "name": "txread",
  "version": "0.0.2",
  "description": "终端阅读工具 - 支持WebDAV同步的txt文件阅读器",
  "main": "dist/index.js",
  "bin": {
    "txread": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "publish:all": "./publish.sh",
    "publish:test": "./test-publish.sh",
    "publish:check": "./check-permissions.sh"
  },
  "keywords": [
    "terminal",
    "reader",
    "webdav",
    "txt",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^9.4.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "progress": "^2.0.3",
    "webdav": "^4.11.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.5",
    "@types/progress": "^2.0.7",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}